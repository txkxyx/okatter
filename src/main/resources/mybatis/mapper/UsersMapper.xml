<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.open.learning.presentation.mapper.UsersMapper">
    <select id="selectByAddress" resultType="com.open.learning.presentation.model.Users">
        select
        *
        from users
        where address = #{address}
    </select>
</mapper>